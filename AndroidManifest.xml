<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mb.kids_mind"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="19" />
  <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
  
    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
          android:largeHeap="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity
            android:name="com.mb.kids_mind.Splash"
            android:noHistory="true" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mb.kids_mind.MainActivity" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".KidsMindDrawActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".KidsMindAnalyzeActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".KidsMindResultActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".KidsMindTotalResultActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".KidsMindAddActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="KidsMindLoginActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>
